public class Main
{
	public static void main(String[] args) {
		int n=6,div=1;
		for(int i=n;i>1;i=i/10)
		{
		    div=div*10;
		}
	    int res=n*n;
	    int d=res%div;
		if(d==n)
		{
		    System.out.println("The given number is sunny number.");
		}
		else{
		    System.out.println("The given number is not a sunny number.");
		}
	
	}
}
